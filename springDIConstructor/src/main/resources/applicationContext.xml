<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="a1" class="com.example.demo.Address">
        <constructor-arg value="MohisKhuja"></constructor-arg>
        <constructor-arg value="Aditmari"></constructor-arg>
        <constructor-arg value="lalmonirhat"></constructor-arg>
    </bean>


    <bean id="e" class="com.example.demo.Employee">
        <constructor-arg value="12"></constructor-arg>
        <constructor-arg value="Shamim"></constructor-arg>
        <constructor-arg>
            <ref bean="a1"/>
        </constructor-arg>

    </bean>

</beans>